            "linkId": "<<macro",
            "address": "auto",
            "preArgs": "",
            "typeTags": "",
            "decimals": 2,
            "target": "",
            "ignoreDefaults": false,
            "bypass": false,
            "onCreate": "",
            "onValue": "const resolveCommands = (macro) => {\n  const msgs = [];\n  \n  for (const subcmd of macro) {\n    \n    if (typeof subcmd === 'object') {\n      msgs.push(subcmd)\n      \n    }\n    else if (subcmd.startsWith(\"/\")) {\n      const wId = subcmd.substring(1)\n      const wValue = get(wId)\n      let wAddress = getProp(wId, 'address')\n      \n      if (wAddress && (wAddress.endsWith('/command') || wAddress.endsWith('/plugin'))) {\n        wAddress = wAddress.substring(wAddress.lastIndexOf('/') + 1)\n      }\n      \n      if (Array.isArray(wValue)) {\n        msgs.push(...resolveCommands(wValue));\n      } else if (wValue) {\n        msgs.push(`${wAddress}:${wValue}`);\n      } else {\n        console.warn(`Variable ${subcmd} not found.`);\n        send('localhost:8080', '/NOTIFY', '^circle-exclamation', `Variable ${subcmd} not found.`)\n      }\n    } else {\n      msgs.push(subcmd);\n    }\n  }\n  \n  return msgs;\n  \n  // return typeof macro === 'object' ? macro : macro.split(';')\n  //   .map(subcmd => {\n  //     if (typeof subcmd === 'string') {\n  //       const wId = subcmd.substring(1)\n  //       const wValue = get(wId)\n  //       let wAddress = getProp(wId, 'address')\n        \n  //       if (wAddress && (wAddress.endsWith('/command') || wAddress.endsWith('/plugin'))) {\n  //         wAddress = wAddress.substring(wAddress.lastIndexOf('/') + 1)\n  //       }\n  //       else {\n  //         return subcmd\n  //       }\n        \n  //       if (Array.isArray(wValue)) {\n  //         return [...resolveCommands(wValue)]\n  //       } else if (wValue) {\n  //         return `${wAddress}:${wValue}`\n  //       } else {\n  //         console.warn(`Variable ${subcmd} not found.`);\n  //         send('localhost:8080', '/NOTIFY', '^circle-exclamation', `Variable ${subcmd} not found.`)\n  //         return\n  //       }\n  //     }\n  //   })\n  //   .filter(subcmd => subcmd)\n}\n\nconst resolvedArray = resolveCommands(value.split(';'));\nsend('localhost:8080','/SibeliusConnect',...resolvedArray)\n",
