            "ignoreDefaults": false,
            "bypass": false,
            "onCreate": "",
            "onValue": "const callback = JSON.parse(value)\nconst type = callback.d.eventType || callback.d.requestType;\nconst data = callback.d.eventData || callback.d.responseData;\n\nswitch (type) {\n  case 'InputVolumeChanged':\n  case 'GetInputVolume':\n    set(data.inputUuid || callback.d.requestId, data.inputVolumeDb, { send: false })\n    break;\n  case 'GetSceneList':\n  case 'SceneListChanged':\n    const scenes = { labels: [], values: []}\n    data.scenes.reverse()\n      .forEach((scene, i) => {\n        scenes.labels[i] = scene.sceneName;\n        scenes.values[i] = scene.sceneUuid;\n      });\n    ['Preview', 'Program'].forEach(m => {\n      setVar(`Current${m}Scene`, 'scenes', scenes);\n      if (data[`current${m}SceneUuid`]) {\n        set(`Current${m}Scene`, data[`current${m}SceneUuid`], { send: false });\n      }\n    })\n    break;\n  case 'CurrentPreviewSceneChanged':\n  case 'CurrentProgramSceneChanged':\n    set(type.replace('Changed', ''), data.sceneUuid, { send: false })\n    break;\n  case 'GetInputList':\n    const inputs = data.inputs\n      .filter(input => input.inputKind.includes(\"audio\"))\n      .map(input => {\n        const msg = {\n          \"op\": 6,\n          \"d\": {\n            \"requestType\": \"GetInputVolume\",\n            \"requestId\": input.inputUuid,\n            \"requestData\": {\n              \"inputUuid\": input.inputUuid\n            }\n          }\n        }\n        send('localhost:8080', '/OBSWebsocket', msg)\n        \n        return {\n          label: input.inputName,\n          id: input.inputUuid,\n          range: {\n            \"min\": { \"-∞\": -95 },\n            \"max\": 0\n          },\n          logScale: .1,\n        }\n      });\n    setVar('InputList', 'inputs', inputs);\n    set('InputListLabels', inputs);\n    break;\n  default:\n    console.log(callback);\n    break;\n}\n",
